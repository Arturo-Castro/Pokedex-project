{
    "env": {
        "browser": true,
        "es2021": true,
        "extends": [
            "plugin:cypress/recommended"
          ]
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-param-reassign": 0,
        "import/extensions": ["warn", {"js": "always"}]
    }
}
